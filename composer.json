{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.2.3",
		"nette/assets": "^1.0.0",
		"nette/bootstrap": "^3.2.6",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^2",
		"symfony/thanks": "^1",
		"laravel/pint": "^1.18"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"scripts": {
		"post-install-cmd": "chmod 777 temp && chmod 777 log",
		"dev": [
			"Composer\\Config::disableProcessTimeout",
			"export NETTE_DEBUG=true && php -S localhost:8000 -t www"
		],
		"preview": [
			"Composer\\Config::disableProcessTimeout",
			"export NETTE_DEBUG=false && php -S localhost:8000 -t www"
		],
		"pint": "pint",
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
